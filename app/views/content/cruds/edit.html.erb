<div class="form-style-2-heading">Update <%= @content.class %>  </div>  
<%= link_to 'Go Back to Content List', cruds_path(model: @content.class) %>

<div class="form-style-2">
  <%= form_with model: @content, url: crud_path,  method: :put do |f| %> 
    <%= render "form", content: @content, f: f %>
  <% end %>
</div>